{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": "error",
    "no-new": "off",
    "no-param-reassign": "off",
    "no-undef": "off"
  }
}
